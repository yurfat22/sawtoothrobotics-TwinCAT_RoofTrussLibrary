    <Method Name="Jog" Id="{01e5017e-389e-4b57-9256-54bd66bd446e}" FolderPath="Methods\">
      <Declaration><![CDATA[METHOD Jog : BOOL
VAR_INPUT
    JogFW : BOOL := FALSE;
    JogBW : BOOL := FALSE;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF JogFW THEN
    Jog    := FALSE;
    JogFwd := TRUE;
    JogRvs := FALSE;

    IF FwdActive AND AtSpeed THEN
        Jog := TRUE;
    END_IF
ELSIF JogBW THEN

    Jog    := FALSE;
    JogFwd := FALSE;
    JogRvs := TRUE;

    IF RvsActive AND AtSpeed THEN
        Jog := TRUE;
    END_IF

END_IF]]></ST>
      </Implementation>
    </Method>
