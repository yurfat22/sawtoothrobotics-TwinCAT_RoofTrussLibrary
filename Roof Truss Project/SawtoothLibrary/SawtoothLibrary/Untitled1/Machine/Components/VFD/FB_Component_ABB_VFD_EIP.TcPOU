VAR

    // Outputs
    _ControlWord  AT %Q* : UDINT;
    _EnableMotor  AT %Q* : BOOL;
    _FaultReset   AT %Q* : BOOL;
    _Frequency    AT %Q* : WORD;
    _JogFwd       AT %Q* : BOOL;
    _JogRvs       AT %Q* : BOOL;
    _ReferenceVel AT %Q* : INT;

    // Inputs
